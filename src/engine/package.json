{
  "name": "lendpeak-engine",
  "version": "1.0.0",
  "main": "./dist/index.js",
  "module": "./dist/esm/index.js",
  "exports": {
    "./models/Amortization": {
      "require": "./dist/commonjs/models/Amortization.js",
      "import": "./dist/esm/models/Amortization.js"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "description": "LendPeak - Open-source lending engine",
  "scripts": {
    "test": "jest",
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc --project tsconfig.esm.json",
    "build:cjs": "tsc --project tsconfig.commonjs.json",
    "build:all": "npm run build:esm && npm run build:cjs",
    "watch:esm": "tsc --project tsconfig.esm.json --watch",
    "watch:cjs": "tsc --project tsconfig.commonjs.json --watch",
    "watch:all": "npm-run-all --parallel watch:esm watch:cjs"
  },
  "keywords": [],
  "author": "",
  "license": "MPL-2.0",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^22.4.2",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "dayjs": "^1.11.13",
    "decimal.js": "^10.4.3"
  }
}